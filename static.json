{
  "root": "dist/",
  "clean_urls": true,
  "https_only": true,
  "headers": {
    "/": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/index.html": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000, immutable"
    },
    "/manifest.webmanifest": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/sw.js": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/registerSW.js": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    }
  }
}